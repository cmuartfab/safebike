 155:../../src/radio/rf231_soc/source/basic_rf.c **** 		continue;
 156:../../src/radio/rf231_soc/source/basic_rf.c **** 	/* For some reason comparing to SLEEP doesn't work, but 0 does */
 157:../../src/radio/rf231_soc/source/basic_rf.c **** 	status = (TRX_STATUS & 0x1F);
 158:../../src/radio/rf231_soc/source/basic_rf.c **** 	if((status != 0) && (status != 0xF))
 159:../../src/radio/rf231_soc/source/basic_rf.c **** 		return;
 160:../../src/radio/rf231_soc/source/basic_rf.c **** 
 161:../../src/radio/rf231_soc/source/basic_rf.c **** 	/* Wake up */
 162:../../src/radio/rf231_soc/source/basic_rf.c **** 	TRXPR &= ~(1 << SLPTR);
