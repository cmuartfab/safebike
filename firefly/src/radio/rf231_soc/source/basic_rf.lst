 992:../../src/radio/rf231_soc/source/basic_rf.c **** 	uint8_t i, j;
 993:../../src/radio/rf231_soc/source/basic_rf.c **** 	uint16_t packets_received = 0;
 994:../../src/radio/rf231_soc/source/basic_rf.c **** 	for (i = 0; i < 128; i++) {
 995:../../src/radio/rf231_soc/source/basic_rf.c **** 		for (j = 0; j < 8; j++) {
 996:../../src/radio/rf231_soc/source/basic_rf.c **** 			packets_received += (packet_flags[i] >> j) & 1;
 997:../../src/radio/rf231_soc/source/basic_rf.c **** 		}
 998:../../src/radio/rf231_soc/source/basic_rf.c **** 	}
 999:../../src/radio/rf231_soc/source/basic_rf.c **** 	return packets_received;
