 125:../../src/radio/rf231_soc/source/basic_rf.c **** void rf_power_down()
 126:../../src/radio/rf231_soc/source/basic_rf.c **** {
 127:../../src/radio/rf231_soc/source/basic_rf.c **** 	uint8_t status;
 128:../../src/radio/rf231_soc/source/basic_rf.c **** 
 129:../../src/radio/rf231_soc/source/basic_rf.c **** 	while((TRX_STATUS & 0x1F) == STATE_TRANSITION_IN_PROGRESS)
 130:../../src/radio/rf231_soc/source/basic_rf.c **** 		continue;
 131:../../src/radio/rf231_soc/source/basic_rf.c **** 
 132:../../src/radio/rf231_soc/source/basic_rf.c **** 	/* For some reason comparing to SLEEP doesn't work, but 0 does */
 133:../../src/radio/rf231_soc/source/basic_rf.c **** 	status = (TRX_STATUS & 0x1F);
 134:../../src/radio/rf231_soc/source/basic_rf.c **** 	if((status == 0) || (status == 0xF))
 135:../../src/radio/rf231_soc/source/basic_rf.c **** 		return;
 136:../../src/radio/rf231_soc/source/basic_rf.c **** 	/* Disable TRX if it is enabled */
 137:../../src/radio/rf231_soc/source/basic_rf.c **** 	if((TRX_STATUS & 0x1F) != TRX_OFF){
 138:../../src/radio/rf231_soc/source/basic_rf.c **** 		rf_cmd(TRX_OFF);
 139:../../src/radio/rf231_soc/source/basic_rf.c **** 		do{
 140:../../src/radio/rf231_soc/source/basic_rf.c **** 			status = (TRX_STATUS & 0x1F);
 141:../../src/radio/rf231_soc/source/basic_rf.c **** 		}while(status != TRX_OFF);
 142:../../src/radio/rf231_soc/source/basic_rf.c **** 	}
 143:../../src/radio/rf231_soc/source/basic_rf.c **** 
 144:../../src/radio/rf231_soc/source/basic_rf.c **** 	TRXPR |= (1 << SLPTR);
 145:../../src/radio/rf231_soc/source/basic_rf.c **** 	do{
 146:../../src/radio/rf231_soc/source/basic_rf.c **** 		status = (TRX_STATUS & 0x1F);
 147:../../src/radio/rf231_soc/source/basic_rf.c **** 	}while((status != 0) && (status != 0xF));
 148:../../src/radio/rf231_soc/source/basic_rf.c **** }
